if ($('#support-name' + idNum).text() == basicArr[player][tempSupp])
  basicArr[player].splice(tempSupp, 1);
else if ($('#support-name' + idNum).text() == epicArr[player][tempSupp])
  epicArr[player].splice(tempSupp, 1);
else
  console.log("Error replacing Supp: $('#support-name' + idNum).text() = " + $('#support-name' + idNum).text() + "basicArr[player][tempSupp]/ epic = " basicArr[player][tempSupp] + " " + epicArr[player][tempSupp]);

